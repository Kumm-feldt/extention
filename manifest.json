{
    "manifest_version": 3,
    "name": "Quote Saver",
    "version": "1.0",
  "permissions": ["storage", "activeTab", "scripting","tabs","storage","fontSettings","contentSettings","debugger"],
    "host_permissions": ["http://*/*","https://*/*"],
    "action": {
        "default_icon":"icon.png",
      "default_popup": "popup.html"
    }
  }
  